{
  "name": "investment-scripts",
  "version": "1.0.0",
  "description": "Скрипты для анализа инвестиционных данных",
  "scripts": {
    "build": "tsc",
    "filter-dividends": "ts-node dividend-filter.ts",
    "scrape-yahoo": "ts-node yahoo-finance.ts",
    "send-to-telegram": "ts-node send-to-telegram.ts",
    "start": "echo 'Пожалуйста, используйте один из следующих скриптов: npm run filter-dividends или npm run scrape-yahoo'",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.{ts,json,md}\"",
    "prepare": "husky install"
  },
  "keywords": [
    "dividends",
    "stocks",
    "filter",
    "excel",
    "yahoo-finance"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.8.2",
    "date-fns": "^4.1.0",
    "exceljs": "^4.3.0",
    "form-data": "^4.0.0",
    "google-auth-library": "^9.15.1",
    "googleapis": "^146.0.0",
    "playwright": "^1.35.0",
    "yahoo-finance2": "^2.13.3"
  },
  "devDependencies": {
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
